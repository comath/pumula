<p id="notice"><%= notice %></p>

<h1>Listing Student Attempts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_attempts.each do |student_attempt| %>
      <tr>
        <td><%= link_to 'Show', student_attempt %></td>
        <td><%= link_to 'Edit', edit_student_attempt_path(student_attempt) %></td>
        <td><%= link_to 'Destroy', student_attempt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student attempt', new_student_attempt_path %>
