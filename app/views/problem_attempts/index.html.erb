<p id="notice"><%= notice %></p>

<h1>Listing Problem Attempts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @problem_attempts.each do |problem_attempt| %>
      <tr>
        <td><%= link_to 'Show', problem_attempt %></td>
        <td><%= link_to 'Edit', edit_problem_attempt_path(problem_attempt) %></td>
        <td><%= link_to 'Destroy', problem_attempt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Problem attempt', new_problem_attempt_path %>
